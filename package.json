  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "vue": {
    "publicPath": "/",
    "css": {
      "loaderOptions": {
        "sass": {
          "prependData": "@import '~@/assets/sass/app'"
        }
      }
    }
  }
